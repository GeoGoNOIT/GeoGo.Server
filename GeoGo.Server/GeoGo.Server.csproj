    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>aspnet-GeoGo.Server-F98CB8BC-BFC3-41DB-8642-ED501F28F594</UserSecretsId>
